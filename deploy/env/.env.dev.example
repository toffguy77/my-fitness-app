# Development environment file for VPS deployment
# Copy to: ${DEPLOY_PATH}/.env.dev
# IMPORTANT: do not commit the real .env.dev (secrets) into git.

# App
NODE_ENV=production
NEXT_PUBLIC_APP_URL=https://dev.burcev.team

# Optional logging
NEXT_PUBLIC_LOG_LEVEL=DEBUG
LOG_LEVEL=DEBUG
NEXT_PUBLIC_DEBUG_MODE=true
DEBUG_MODE=true
NEXT_PUBLIC_ENABLE_USER_FLOW_LOGGING=true
ENABLE_USER_FLOW_LOGGING=true

# Supabase (dev project)
NEXT_PUBLIC_SUPABASE_URL=your_dev_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_dev_supabase_anon_key

# Email (Resend)
RESEND_API_KEY=re_xxxxx
RESEND_FROM_EMAIL=BURCEV Dev <noreply@burcev.team>

# OCR (optional)
NEXT_PUBLIC_OPENROUTER_API_KEY=sk-or-v1-xxxxx
