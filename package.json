{
  "name": "my-fitness-app",
  "version": "0.15.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "test": "NODE_OPTIONS=--max-old-space-size=4096 jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:coverage:ci": "jest --coverage --ci --watchAll=false --passWithNoTests",
    "test:coverage:threshold": "jest --coverage --coverageThreshold",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test && npm run test:e2e",
    "quality-gate": "node scripts/quality-gate-incremental.js",
    "quality-gate:performance": "node scripts/quality-gate-performance.js",
    "quality-gate:optimize": "node scripts/quality-gate-optimizer.js optimize",
    "quality-gate:analyze": "node scripts/quality-gate-optimizer.js analyze",
    "quality-gate:fast": "npm run quality-gate",
    "quality-gate:full": "npm run quality-gate:performance",
    "perf:monitor": "node scripts/pipeline-monitor.js",
    "perf:report": "node scripts/pipeline-monitor.js report",
    "perf:dashboard": "node scripts/performance-dashboard.js generate",
    "perf:optimize": "node scripts/performance-optimizer.js",
    "perf:alerts": "node scripts/pipeline-monitor.js alerts",
    "perf:cleanup": "node scripts/pipeline-monitor.js cleanup",
    "security:scan": "node scripts/security-scanner.js all",
    "security:dependencies": "node scripts/security-scanner.js dependencies",
    "security:sast": "node scripts/security-scanner.js sast",
    "security:secrets": "node scripts/security-scanner.js secrets",
    "security:licenses": "node scripts/security-scanner.js licenses",
    "security:audit": "npm audit --audit-level high",
    "security:fix": "npm audit fix",
    "lint:security": "eslint . --config eslint.security.config.mjs --ext .js,.jsx,.ts,.tsx",
    "precommit:setup": "bash scripts/setup-pre-commit.sh",
    "precommit:run": "pre-commit run --all-files",
    "precommit:update": "pre-commit autoupdate"
  },
  "dependencies": {
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.87.3",
    "@types/papaparse": "^5.5.2",
    "clsx": "^2.1.1",
    "cross-spawn": "^7.0.6",
    "glob": "^10.5.0",
    "jspdf": "^4.0.0",
    "lucide-react": "^0.561.0",
    "next": "16.0.10",
    "next-pwa": "^5.6.0",
    "papaparse": "^5.5.3",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.6.0",
    "resend": "^4.0.0",
    "supabase": "^2.67.2",
    "tailwind-merge": "^3.4.0",
    "tesseract.js": "^5.1.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "eslint-plugin-no-secrets": "^1.0.2",
    "eslint-plugin-security": "^3.0.1",
    "fast-check": "^4.5.3",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "js-yaml": "^4.1.1",
    "msw": "^2.12.4",
    "playwright": "^1.57.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "typescript": "^5"
  }
}