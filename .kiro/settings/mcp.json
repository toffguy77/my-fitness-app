{
    "mcpServers": {
        "supabase": {
            "command": "/opt/homebrew/bin/uvx",
            "args": [
                "supabase-mcp-server@latest",
                "https://mcp.supabase.com/mcp?project_ref=kvdusetwlsxcusbjwvdu&read_only=false"
            ],
            "env": {
                "FASTMCP_LOG_LEVEL": "ERROR"
            },
            "disabled": false,
            "autoApprove": []
        },
        "git": {
            "command": "/opt/homebrew/bin/uvx",
            "args": [
                "mcp-server-git",
                "--repository",
                "/Users/thatguy/src/my-fitness-app"
            ],
            "autoApprove": [
                "git_diff_unstaged",
                "git_status",
                "git_diff_staged",
                "git_log"
            ]
        }
    }
}
